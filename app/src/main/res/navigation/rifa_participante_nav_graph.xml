<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/rifa_participante_nav_graph"
    app:startDestination="@id/rifaParticipanteFragment">
    <fragment
        android:id="@+id/rifaParticipanteFragment"
        android:name="com.emmanuel.chancita.ui.rifa.RifaParticipanteFragment"
        android:label="fragment_rifa_participante"
        tools:layout="@layout/fragment_rifa_participante" >

        <argument
            android:name="rifa_id"
            app:argType="string" />
        <action
            android:id="@+id/action_rifaParticipanteFragment_to_qrRifaFragment"
            app:destination="@id/qrRifaFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/slide_out_left"
            app:popEnterAnim="@anim/slide_in_left"
            app:popExitAnim="@anim/slide_out_right"
            />
    </fragment>
    <fragment
        android:id="@+id/qrRifaFragment"
        android:name="com.emmanuel.chancita.ui.rifa.QrRifaFragment"
        android:label="QrRifaFragment" />
</navigation>